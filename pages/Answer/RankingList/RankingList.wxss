/* 定义一个容器 */
.main{
  margin: 10rpx 50rpx ;
}
/* 分享属性设置 */
.shareButton{
  width: 100rpx;
  height: 50rpx;
  display: flex;
  flex-direction: row;
}
/* 分享属性文本设置 */
.shareButton text{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
}
/* 头部属性设置 */
.head{
  display: flex;
  border-bottom: 2rpx solid #cccccc;
  padding-bottom: 20rpx;
}
/*排行榜前50文本设置  */
.head .head50{
  flex: 2;
  display: flex;
  justify-content: flex-end;
}
/* 分享属性设置 */
.head .share{
  flex: 1;
  display: flex;
  justify-content: flex-end;
}
/* 列表 */
.oneList{
  display: flex;
  margin:10rpx 20rpx;
  font-weight: bold;
  font-size: 25rpx;
  height: 120rpx;
  align-content: center;
  justify-content: center;
  border-bottom: 2rpx solid #cccccc;  
}
.oneList .rankingNumber{
  flex: 1;
  display: flex;
  font-size: 30rpx;
  justify-content:center;
  align-items: center;
}
.oneList .headimg{
  flex: 2;
}
.oneList .headimg image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}
.oneList .information{
  flex: 4;
}
.oneList .information .username{
  margin-bottom: 20rpx;
}
.position{
  width: 20rpx;
  height: 20rpx;
}
.oneList .grade{
  flex: 1;
}
.oneList .sorceNumber{
  flex: 2;
  display: flex;
  color: #eccb28;
  justify-content:flex-end;
  align-items: center;
}
.duiq{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}